generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}
model Task {
  id        String   @id @default(cuid())
  title     String
  description String?
  dueAt     DateTime
  isDone    Boolean  @default(false)
  createdAt DateTime @default(now())
  updatedAt DateTime?
}

